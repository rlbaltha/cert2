{% extends 'base.html.twig' %}

{% block title %}Checklist{% endblock %}

{% block body %}
    <h1>Checklist</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ checklist.id }}</td>
            </tr>
            <tr>
                <th>Presentation</th>
                <td>{{ checklist.presentation }}</td>
            </tr>
            <tr>
                <th>Athena</th>
                <td>{{ checklist.athena ? checklist.athena|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Pre_assess</th>
                <td>{{ checklist.preAssess ? checklist.preAssess|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Post_assess</th>
                <td>{{ checklist.postAssess ? checklist.postAssess|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Orientation</th>
                <td>{{ checklist.orientation ? checklist.orientation|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Appliedtograd</th>
                <td>{{ checklist.appliedtograd }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('checklist_index') }}">back to list</a>

    <a href="{{ path('checklist_edit', {'id': checklist.id}) }}">edit</a>

    {{ include('checklist/_delete_form.html.twig') }}
{% endblock %}
