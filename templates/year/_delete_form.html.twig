<hr/>
<form class="d-inline float-right" method="post" action="{{ path('year_delete', {'id': year.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ year.id) }}">
    <button class="btn btn-danger">Delete</button>
</form>
